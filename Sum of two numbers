package com.keerthi;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
     Scanner in=new Scanner(System.in);
        System.out.println("enter a number: ");
        int num=in.nextInt();
        System.out.println("enter a number: ");
        int num1=in.nextInt();
        while(num1!=0){
         num++;
         num1--;
            
     }
          
        System.out.println("\nsum of two numbers: "+num);
      
    }
}
